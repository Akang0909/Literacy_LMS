@model List<BorrowedBook>

<h2>Previously Borrowed Books</h2>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Book ID</th>
            <th>Book Name</th>
            <th>Issue Date</th>
            <th>Return Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var book in Model)
        {
            <tr>
                <td>@book.BookId</td>
                <td>@book.BookName</td>
                <td>@book.IssueDate.ToString("yyyy-MM-dd")</td>
                <td>@book.ReturnDate?.ToString("yyyy-MM-dd")</td>
            </tr>
        }
    </tbody>
</table>
